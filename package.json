{
  "name": "nestjs-async-config",
  "version": "0.1.3",
  "description": "The async config module based on RESTful API, you can real time update your system config",
  "author": "Terence.Sun",
  "main": "dist/index.js",
  "license": "MIT",
  "directories": {
    "lib": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nayo-project/nestjs-async-config.git"
  },
  "bugs": {
    "url": "https://github.com/nayo-project/nestjs-async-config/issues"
  },
  "homepage": "https://github.com/nayo-project/nestjs-async-config#readme",
  "scripts": {
    "build": "tsc -p tsconfig.build.json"
  },
  "keywords": [
    "nestjs",
    "config",
    "environment",
    "async",
    "RESTful",
    "module",
    "update config",
    "real time update",
    "real time"
  ],
  "dependencies": {
    "@nestjs/core": "8.2.4",
    "@types/yaml": "1.2.0",
    "app-root-path": "^3.0.0",
    "ioredis": "4.14.1",
    "mongoose": "6.0.12",
    "nestjs-validate": "0.5.7",
    "redis": "3.1.2",
    "rxjs": "7.2.0",
    "yaml": "1.7.2"
  },
  "devDependencies": {
    "@nestjs/common": "8.1.2",
    "@nestjs/swagger": "^5.0.8",
    "@nestjs/testing": "8.0.0",
    "@types/node": "15.6.1",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "4.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
